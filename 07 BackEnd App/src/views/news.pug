extends layout

block contentBlock
  h1= title

  h2 Nasze News'y

  form(method='get')
    input(type='text' value=search name='search')
    input(type='submit' value='Szukaj')

  each item in data 
    article
      h1(class='title')=item.title
      p(class='date')=item.created
      p=item.description
